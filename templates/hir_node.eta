<% if (it.node.type === "link") { %>
	<p class="text-lg"><a class="link" href="<%= it.node.href %>"><%= it.node.name %></a></p>
<% } else if (it.node.type === "group") { %>
	<div class="basis-full">
		<%~ include("./group.eta", { group: it.node }) %>
	</div>
<% } %>
